{"components/account/edit-account":"<form action=\"{{urls.account.update_action}}\" data-edit-account-form class=\"form\" method=\"post\">\r\n    {{inject 'firstName' (lang 'forms.validate.account.edit.first_name')}}\r\n    {{inject 'lastName' (lang 'forms.validate.account.edit.last_name')}}\r\n    {{inject 'phoneNumber' (lang 'forms.validate.account.edit.phone')}}\r\n    {{inject 'currentPassword' (lang 'forms.validate.account.edit.password')}}\r\n    <fieldset class=\"form-fieldset\">\r\n        <div class=\"form-row form-row--half\">\r\n            <div class=\"form-field\">\r\n                <label class=\"form-label\" for=\"account_firstname\">\r\n                    {{lang 'account.settings.first_name' }}\r\n                    <small>{{lang 'common.required' }}</small>\r\n                </label>\r\n                <input type=\"text\" class=\"form-input\" name=\"account_firstname\" id=\"account_firstname\" value=\"{{forms.edit_account.first_name}}\">\r\n            </div>\r\n            <div class=\"form-field\">\r\n                <label class=\"form-label\" for=\"account_lastname\">\r\n                    {{lang 'account.settings.last_name' }}\r\n                    <small>{{lang 'common.required' }}</small>\r\n                </label>\r\n                <input type=\"text\" class=\"form-input\" name=\"account_lastname\" id=\"account_lastname\" value=\"{{forms.edit_account.last_name}}\">\r\n            </div>\r\n            <div class=\"form-field\">\r\n                <label class=\"form-label\" for=\"account_companyname\">\r\n                    {{lang 'account.settings.company' }}\r\n                </label>\r\n                <input type=\"text\" class=\"form-input\" name=\"account_companyname\" id=\"account_companyname\" value=\"{{forms.edit_account.company_name}}\">\r\n            </div>\r\n            <div class=\"form-field\">\r\n                <label class=\"form-label\" for=\"account_phone\">\r\n                    {{lang 'account.settings.phone' }}\r\n                </label>\r\n                <input type=\"text\" class=\"form-input\" name=\"account_phone\" id=\"account_phone\" value=\"{{forms.edit_account.phone}}\">\r\n            </div>\r\n            {{#each forms.edit_account.fields}}\r\n                {{{dynamicComponent 'components/common/forms'}}}\r\n            {{/each}}\r\n        </div>\r\n    </fieldset>\r\n    <div class=\"form-actions\">\r\n        <button class=\"button button--primary\">{{lang 'account.settings.update_details'}}</button>\r\n    </div>\r\n</form>\r\n","components/account/navigation":"<nav class=\"navBar navBar--sub navBar--account\">\r\n    <ul class=\"navBar-section\">\r\n        {{#if account_page '===' 'orders'}}\r\n            {{#if page_type '===' 'account_order'}}\r\n                <li class=\"navBar-item is-active\">\r\n                    <a class=\"navBar-action\" href=\"{{urls.account.orders.all}}\">{{lang 'account.nav.orders'}}</a>\r\n                </li>\r\n            {{else}}\r\n                <li class=\"navBar-item is-active\">{{lang 'account.nav.orders'}}</li>\r\n            {{/if}}\r\n        {{else}}\r\n            <li class=\"navBar-item\">\r\n                <a class=\"navBar-action\" href=\"{{urls.account.orders.all}}\">{{lang 'account.nav.orders'}}</a>\r\n            </li>\r\n        {{/if}}\r\n        {{#if settings.returns_enabled}}\r\n            {{#if account_page '===' 'returns'}}\r\n                <li class=\"navBar-item is-active\">{{lang 'account.nav.returns'}}</li>\r\n            {{else}}\r\n                <li class=\"navBar-item\">\r\n                    <a class=\"navBar-action\" href=\"{{urls.account.returns}}\">{{lang 'account.nav.returns'}}</a>\r\n                </li>\r\n            {{/if}}\r\n        {{/if}}\r\n        {{#if account_page '===' 'messages'}}\r\n            <li class=\"navBar-item is-active\">{{lang 'account.nav.messages' num_new_messages=customer.num_new_messages}}</li>\r\n        {{else}}\r\n            <li class=\"navBar-item\">\r\n                <a class=\"navBar-action\" href=\"{{urls.account.inbox}}\">{{lang 'account.nav.messages' num_new_messages=customer.num_new_messages}}</a>\r\n            </li>\r\n        {{/if}}\r\n        {{#if account_page '===' 'addresses'}}\r\n            <li class=\"navBar-item is-active\">{{lang 'account.nav.addresses'}}</li>\r\n        {{else}}\r\n            <li class=\"navBar-item\">\r\n                <a class=\"navBar-action\" href=\"{{urls.account.addresses}}\">{{lang 'account.nav.addresses'}}</a>\r\n            </li>\r\n        {{/if}}\r\n        {{#if settings.show_payment_methods}}\r\n            {{#if account_page '===' 'payment_methods'}}\r\n                <li class=\"navBar-item is-active\">{{lang 'account.nav.payment_methods'}}</li>\r\n            {{else}}\r\n                <li class=\"navBar-item\">\r\n                    <a class=\"navBar-action\" href=\"{{urls.account.payment_methods.all}}\">{{lang 'account.nav.payment_methods'}}</a>\r\n                </li>\r\n            {{/if}}\r\n        {{/if}}\r\n        {{#if settings.show_wishlist}}\r\n            {{#if account_page '===' 'wishlists'}}\r\n                <li class=\"navBar-item is-active\">{{lang 'account.nav.wishlists' num_wishlists=customer.num_wishlists}}</li>\r\n            {{else}}\r\n                <li class=\"navBar-item\">\r\n                    <a class=\"navBar-action\" href=\"{{urls.account.wishlists.all}}\">{{lang 'account.nav.wishlists' num_wishlists=customer.num_wishlists}}</a>\r\n                </li>\r\n            {{/if}}\r\n        {{/if}}\r\n        {{#if account_page '===' 'recent_items'}}\r\n            <li class=\"navBar-item is-active\">{{lang 'account.nav.recently_viewed'}}</li>\r\n        {{else}}\r\n            <li class=\"navBar-item\">\r\n                <a class=\"navBar-action\" href=\"{{urls.account.recent_items}}\">{{lang 'account.nav.recently_viewed'}}</a>\r\n            </li>\r\n        {{/if}}\r\n        {{#if account_page '===' 'settings'}}\r\n            <li class=\"navBar-item is-active\">{{lang 'account.nav.settings'}}</li>\r\n        {{else}}\r\n            <li class=\"navBar-item\">\r\n                <a class=\"navBar-action\" href=\"{{urls.account.details}}\">{{lang 'account.nav.settings'}}</a>\r\n            </li>\r\n        {{/if}}\r\n    </ul>\r\n</nav>\r\n","components/common/alert-error":"<div class=\"alertBox alertBox--error\">\r\n    <div class=\"alertBox-column alertBox-icon\">\r\n        <icon glyph=\"ic-error\" class=\"icon\" aria-hidden=\"true\"><svg xmlns=\"http://www.w3.org/2000/svg\" width=\"24\" height=\"24\" viewBox=\"0 0 24 24\"><path d=\"M12 2C6.48 2 2 6.48 2 12s4.48 10 10 10 10-4.48 10-10S17.52 2 12 2zm1 15h-2v-2h2v2zm0-4h-2V7h2v6z\"></path></svg></icon>\r\n    </div>\r\n    <p class=\"alertBox-column alertBox-message\">\r\n        <span>{{{this}}}</span>\r\n    </p>\r\n</div>\r\n","components/common/alert-modal":"<div id=\"alert-modal\" class=\"modal modal--alert modal--small\" data-reveal data-prevent-quick-search-close>\r\n    <div class=\"swal2-icon swal2-error swal2-animate-error-icon\"><span class=\"swal2-x-mark swal2-animate-x-mark\"><span class=\"swal2-x-mark-line-left\"></span><span class=\"swal2-x-mark-line-right\"></span></span></div>\r\n\r\n    <div class=\"modal-content\"></div>\r\n\r\n    <div class=\"button-container\"><button type=\"button\" class=\"confirm button\" data-reveal-close>OK</button></div>\r\n</div>\r\n","components/common/alert-success":"<div class=\"alertBox alertBox--success\">\r\n    <div class=\"alertBox-column alertBox-icon\">\r\n        <icon glyph=\"ic-success\" class=\"icon\" aria-hidden=\"true\"><svg xmlns=\"http://www.w3.org/2000/svg\" width=\"24\" height=\"24\" viewBox=\"0 0 24 24\"><path d=\"M12 2C6.48 2 2 6.48 2 12s4.48 10 10 10 10-4.48 10-10S17.52 2 12 2zm1 15h-2v-2h2v2zm0-4h-2V7h2v6z\"></path></svg></icon>\r\n    </div>\r\n    <p class=\"alertBox-column alertBox-message\">\r\n        <span>{{{this}}}</span>\r\n    </p>\r\n</div>\r\n","components/common/breadcrumbs":"<ul class=\"breadcrumbs\" itemscope itemtype=\"http://schema.org/BreadcrumbList\">\r\n    {{#unless theme_settings.hide_breadcrumbs }}\r\n        {{#each breadcrumbs}}\r\n            <li class=\"breadcrumb {{#if @last}}is-active{{/if}}\" itemprop=\"itemListElement\" itemscope itemtype=\"http://schema.org/ListItem\">\r\n                {{#or @last (if url \"==\" null)}}\r\n                    <meta itemprop=\"item\" content=\"{{url}}\">\r\n                    <span class=\"breadcrumb-label\" itemprop=\"name\">{{name}}</span>\r\n                {{else}}\r\n                    <a href=\"{{url}}\" class=\"breadcrumb-label\" itemprop=\"item\"><span itemprop=\"name\">{{name}}</span></a>\r\n                {{/or}}\r\n                <meta itemprop=\"position\" content=\"{{add @index 1}}\" />\r\n            </li>\r\n        {{/each}}\r\n    {{/unless}}\r\n</ul>\r\n","components/common/cookie":"<div class=\"cookieMessage\">\r\n    <div class=\"cookieMessage-container\">\r\n        <div class=\"cookieMessage-text\">\r\n            <p>{{settings.privacy_cookie}}</p>\r\n        </div>\r\n        <button class=\"button button--primary button--small\" data-privacy-accept>{{lang 'cookies.accept'}}</button>\r\n    </div>\r\n</div>\r\n","components/common/forms/checkbox":"<div class=\"form-field\" id=\"{{id}}\" data-validation=\"{{validation}}\" {{#if private_id}}data-type=\"{{private_id}}\"{{/if}}>\r\n    <label class=\"form-label\" for=\"{{id}}\">{{label}}\r\n        {{#if required}}\r\n            <small>{{lang 'common.required' }}</small>\r\n        {{/if}}\r\n    </label>\r\n    {{#if checkboxes}}\r\n        {{#each checkboxes}}\r\n            <input type=\"checkbox\" value=\"{{value}}\" name=\"{{name}}\" id=\"{{name}}\" data-label=\"{{label}}\" class=\"form-checkbox\" {{#if checked}}checked{{/if}} {{#if private_id}}data-field-type=\"{{private_id}}\"{{/if}}>\r\n            <label class=\"form-label {{class}}\" for=\"{{name}}\">{{label}}</label>\r\n        {{/each}}\r\n    {{/if}}\r\n</div>\r\n","components/common/forms/date":"<div class=\"form-field\" id=\"{{id}}\" data-validation=\"{{validation}}\" {{#if private_id}}data-type=\"{{private_id}}\"{{/if}}>\r\n    <label class=\"form-label\" for=\"{{id}}\" >{{label}}\r\n    {{#if required}}<small>{{lang 'common.required' }}</small>{{/if}}</label>\r\n    <div class=\"form-row form-row--third\">\r\n        <div class=\"form-field\">\r\n            <select class=\"form-select\" name=\"{{month.name}}\" id=\"{{id}}_month\" data-label=\"month\"  aria-required=\"{{required}}\" {{#if private_id}}data-field-type=\"{{private_id}}\"{{/if}}>\r\n                {{#each month.items}}\r\n                    <option {{#if ../month.value '==' value}} selected {{/if}}value=\"{{value}}\">{{label}}</option>\r\n                {{/each}}\r\n            </select>\r\n        </div>\r\n        <div class=\"form-field\">\r\n            <select class=\"form-select\" name=\"{{day.name}}\" id=\"{{id}}_day\" data-label=\"day\" aria-required=\"{{required}}\" {{#if private_id}}data-field-type=\"{{private_id}}\"{{/if}}>\r\n                {{#each day.items}}\r\n                    <option {{#if ../day.value '==' value}} selected {{/if}}value=\"{{value}}\">{{label}}</option>\r\n                {{/each}}\r\n            </select>\r\n        </div>\r\n        <div class=\"form-field\">\r\n            <select class=\"form-select\" name=\"{{year.name}}\" id=\"{{id}}_year\" data-label=\"year\" aria-required=\"{{required}}\" {{#if private_id}}data-field-type=\"{{private_id}}\"{{/if}}>\r\n                {{#each year.items}}\r\n                    <option {{#if ../year.value '==' value}} selected {{/if}}value=\"{{value}}\">{{label}}</option>\r\n                {{/each}}\r\n            </select>\r\n        </div>\r\n    </div>\r\n</div>\r\n","components/common/forms/multiline":"<div class=\"form-field\" id=\"{{id}}\" data-validation=\"{{validation}}\">\r\n    <label class=\"form-label\" for=\"{{id}}_input\">{{label}}\r\n        {{#if required}}\r\n            <small>{{lang 'common.required' }}</small>\r\n        {{/if}}\r\n    </label>\r\n    <textarea name=\"{{name}}\" id=\"{{id}}_input\" data-label=\"{{label}}\" rows=\"{{rows}}\" aria-required=\"{{required}}\" class=\"form-input\" {{#if private_id}}data-field-type=\"{{private_id}}\"{{/if}}>{{value}}</textarea>\r\n</div>\r\n","components/common/forms/number":"<div class=\"form-field\" id=\"{{id}}\" data-validation=\"{{validation}}\">\r\n    <label class=\"form-label\" for=\"{{id}}_input\">{{label}}\r\n        {{#if required}}<small>{{lang 'common.required' }}</small>{{/if}}\r\n    </label>\r\n    <input type=\"number\" class=\"form-input\" data-label=\"{{label}}\" data-rule=\"lowerThan\" min=\"{{limitfrom}}\" max=\"{{limitto}}\" title=\"{{lang 'forms.range' limitFrom=limitfrom limitTo=limitto}}\" value=\"{{value}}\" id=\"{{id}}_input\" name=\"{{name}}\" aria-required=\"{{required}}\" {{#if private_id}}data-field-type=\"{{private_id}}\"{{/if}}>\r\n</div>\r\n","components/common/forms/password":"<div class=\"form-field\" id=\"{{id}}\" data-validation=\"{{validation}}\" {{#if private_id}}data-type=\"{{private_id}}\"{{/if}}>\r\n    <label class=\"form-label\" for=\"{{id}}_input\">{{label}}\r\n        {{#if required}}\r\n            <small>{{lang 'common.required' }}</small>\r\n        {{/if}}\r\n    </label>\r\n    <input type=\"password\"  class=\"form-input\" id=\"{{id}}_input\" data-label=\"{{label}}\" name=\"{{name}}\" value=\"{{value}}\" autocomplete=\"off\" aria-required=\"{{required}}\" {{#if private_id}}data-field-type=\"{{private_id}}\"{{/if}}>\r\n</div>\r\n","components/common/forms/radio":"<div class=\"form-field\" id=\"{{id}}\" data-validation=\"{{validation}}\">\r\n    <label class=\"form-label\">{{label}}\r\n        {{#if required}}\r\n            <small>{{lang 'common.required' }}</small>\r\n        {{/if}}\r\n    </label>\r\n    {{#if radios}}\r\n        {{#each radios}}\r\n            <input \r\n                type=\"radio\"\r\n                class=\"form-radio\"\r\n                name=\"{{../name}}\"\r\n                id=\"{{../name}}_{{@index}}\"\r\n                value=\"{{value}}\"\r\n                data-label=\"{{label}}\"\r\n                aria-required=\"{{required}}\"\r\n                {{#if checked}}\r\n                    checked\r\n                {{/if}}\r\n                {{#if private_id}}\r\n                    data-field-type=\"{{private_id}}\"\r\n                {{/if}}>\r\n            <label class=\"form-label\" for=\"{{../name}}_{{@index}}\">{{label}}</label>\r\n        {{/each}}\r\n    {{/if}}\r\n</div>\r\n","components/common/forms/select":"<div class=\"form-field\" id=\"{{id}}\" data-validation=\"{{validation}}\" {{#if private_id}} data-type=\"{{private_id}}\"\r\n    {{/if}}>\r\n    <label class=\"form-label\" for=\"{{id}}_select\">{{label}}\r\n        {{#if required}}<small>{{lang 'common.required' }}</small>{{/if}}\r\n    </label>\r\n    <select class=\"{{class}} form-select\" id=\"{{id}}_select\" data-label=\"{{label}}\" aria-required=\"{{required}}\" name=\"{{name}}\"\r\n        {{#if private_id}} data-field-type=\"{{private_id}}\" {{/if}}>\r\n        {{#if options}}\r\n        <option value=\"\">{{chooseprefix}}</option>\r\n        {{#each options}}\r\n        <option {{#if selected}} selected {{/if}} value=\"{{value}}\">{{{label}}}</option>\r\n        {{/each}}\r\n        {{/if}}\r\n    </select>\r\n</div>\r\n","components/common/forms/selectortext":"<div class=\"form-field\" id=\"{{id}}\" data-validation=\"{{validation}}\" {{#if private_id}}data-type=\"{{private_id}}\"{{/if}}>\r\n    <noscript>\r\n        <label class=\"form-label\" for=\"{{id}}_input\">{{label}}\r\n            {{#if required}}<small>{{lang 'common.required' }}</small>{{/if}}\r\n        </label>\r\n        <input type=\"text\" id=\"{{id}}_input\" name=\"{{name}}\" value=\"{{value}}\" class=\"form-input\" aria-required=\"{{required}}\" data-label=\"{{label}}\" />\r\n    </noscript>\r\n\r\n    <label class=\"form-label\" for=\"{{id}}_select\">{{label}}\r\n        {{#if required}}\r\n            <small>{{lang 'common.required'}}</small>\r\n        {{/if}}\r\n    </label>\r\n    <select name=\"{{name}}\" class=\"form-select\" aria-required=\"{{required}}\" id=\"{{id}}_select\" data-label=\"{{label}}\" {{#if private_id}}data-field-type=\"{{private_id}}\"{{/if}}>\r\n        {{#if options}}\r\n            <option value=\"\">{{chooseprefix}}</option>\r\n            {{#each options}}\r\n                <option {{#if selected}} selected {{/if}}value=\"{{value}}\">{{label}}</option>\r\n            {{/each}}\r\n        {{/if}}\r\n    </select>\r\n</div>\r\n","components/common/forms/text":"<div class=\"form-field\" id=\"{{id}}\" data-validation=\"{{validation}}\" {{#if private_id}}data-type=\"{{private_id}}\"{{/if}}>\r\n    <label class=\"form-label\" for=\"{{id}}_input\">{{label}}\r\n        {{#if required}}<small>{{lang 'common.required' }}</small>{{/if}}\r\n    </label>\r\n    <input type=\"text\" id=\"{{id}}_input\" data-label=\"{{label}}\" name=\"{{name}}\" {{#if value}} value=\"{{value}}\"{{/if}} {{#if placeholder}} placeholder=\"{{placeholder}}\"{{/if}} class=\"form-input\" aria-required=\"{{required}}\" {{#if private_id}}data-field-type=\"{{private_id}}\"{{/if}}>\r\n</div>\r\n","components/common/geotrust-ssl-seal":"<table width=\"135\" border=\"0\" cellpadding=\"2\" cellspacing=\"0\" title=\"Click to Verify - This site chose GeoTrust SSL for secure e-commerce and confidential communications.\">\r\n    <tr>\r\n        <td width=\"135\" align=\"center\" valign=\"top\">\r\n            <script type=\"text/javascript\" src=\"https://seal.geotrust.com/getgeotrustsslseal?host_name={{theme_settings.geotrust_ssl_common_name}}&amp;size={{theme_settings.geotrust_ssl_seal_size}}&amp;lang={{locale_name}}\"></script><br />\r\n            <a href=\"http://www.geotrust.com/ssl/\" target=\"_blank\"  style=\"color:#000000; text-decoration:none; font:bold 7px verdana,sans-serif; letter-spacing:.5px; text-align:center; margin:0px; padding:0px;\"></a>\r\n        </td>\r\n    </tr>\r\n</table>\r\n","components/common/modal":"<div id=\"modal\" class=\"modal\" data-reveal data-prevent-quick-search-close>\r\n    <a href=\"#\" class=\"modal-close\" aria-label=\"{{lang 'common.close'}}\" role=\"button\">\r\n        <span aria-hidden=\"true\">&#215;</span>\r\n    </a>\r\n    <div class=\"modal-content\"></div>\r\n    <div class=\"loadingOverlay\"></div>\r\n</div>\r\n","components/common/payment-icons":"{{#with theme_settings}}\r\n    {{#or show_accept_amex show_accept_discover show_accept_mastercard show_accept_paypal show_accept_visa}}\r\n        <div class=\"footer-payment-icons\">\r\n            {{#if show_accept_amex}}\r\n                <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-american-express\"></use></svg>\r\n            {{/if}}\r\n            {{#if show_accept_discover}}\r\n                <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-discover\"></use></svg>\r\n            {{/if}}\r\n            {{#if show_accept_mastercard}}\r\n                <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-mastercard\"></use></svg>\r\n            {{/if}}\r\n            {{#if show_accept_paypal}}\r\n                <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-paypal\"></use></svg>\r\n            {{/if}}\r\n            {{#if show_accept_visa}}\r\n                <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-visa\"></use></svg>\r\n            {{/if}}\r\n        </div>\r\n    {{/or}}\r\n{{/with}}\r\n","components/common/quick-search":"<div class=\"container\">\r\n    {{{ snippet \"forms_search\"}}}\r\n    <form class=\"form\" action=\"{{urls.search}}\">\r\n        <fieldset class=\"form-fieldset\">\r\n            <div class=\"form-field\">\r\n                <label class=\"is-srOnly\" for=\"search_query\">{{lang \"search.quick_search.input_label\"}}</label>\r\n                <input class=\"form-input\" data-search-quick name=\"search_query\" id=\"search_query\" data-error-message=\"{{lang 'search.error.empty_field'}}\" placeholder=\"{{lang 'search.quick_search.input_placeholder'}}\" autocomplete=\"off\">\r\n            </div>\r\n        </fieldset>\r\n    </form>\r\n    <section class=\"quickSearchResults\" data-bind=\"html: results\"></section>\r\n</div>\r\n","components/common/social-links":"{{#if social_media}}\r\n    <ul class=\"socialLinks socialLinks--alt\">\r\n        {{#each social_media}}\r\n            <li class=\"socialLinks-item\">\r\n                {{#if display_name '===' 'google+'}}\r\n                    <a class=\"icon icon--google\" href=\"{{url}}\" target=\"_blank\">\r\n                        <svg><use xlink:href=\"#icon-google\" /></svg>\r\n                    </a>\r\n                {{else}}\r\n                    <a class=\"icon icon--{{display_name}}\" href=\"{{url}}\" target=\"_blank\">\r\n                        <svg><use xlink:href=\"#icon-{{display_name}}\" /></svg>\r\n                    </a>\r\n                {{/if}}\r\n            </li>\r\n        {{/each}}\r\n    </ul>\r\n{{/if}}\r\n","components/common/store-logo":"<a href=\"{{urls.home}}\">\r\n    {{#if settings.store_logo.image}}\r\n        {{#if theme_settings.logo_size '===' 'original'}}\r\n            <img class=\"header-logo-image-unknown-size\" src=\"{{getImage settings.store_logo.image 'logo_size'}}\" alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\">\r\n        {{else}}\r\n            <div class=\"header-logo-image-container\">\r\n                <img class=\"header-logo-image\" src=\"{{getImage settings.store_logo.image 'logo_size'}}\" alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\">\r\n            </div>\r\n        {{/if}}\r\n    {{else}}\r\n        <span class=\"header-logo-text\">{{settings.store_logo.title}}</span>\r\n    {{/if}}\r\n</a>\r\n","components/ob-custom/ob-body":"<div class=\"body\" data-currency-code=\"{{currency_selector.active_currency_code}}\">\r\n    {{#block \"hero\"}} {{/block}}\r\n    {{#block \"heading-full-width\"}} {{/block}}\r\n    <div class=\"container\">\r\n        {{#block \"page\"}} {{/block}}\r\n    </div>\r\n    {{#block \"full-width\"}} {{/block}}\r\n    \r\n    {{> components/common/modal}}\r\n    {{> components/common/alert-modal}}\r\n</div>\r\n    \r\n","components/ob-custom/ob-currency-selector":"<div>\r\n    <ul class=\"navUser-section\">\r\n        {{#if currency_selector.currencies.length '>' 1}}\r\n          <li class=\"navUser-item\">\r\n              <a class=\"navUser-action has-dropdown\" href=\"javascript:void(0)\" data-dropdown=\"currencySelection\" aria-controls=\"currencySelection\" aria-expanded=\"false\">\r\n                <span>Select Currency:</span> <b>{{lang 'common.currency' code=currency_selector.active_currency_code}}</b>\r\n              </a>\r\n          </li>\r\n        {{/if}}\r\n    </ul>\r\n    \r\n    {{#if currency_selector.currencies.length '>' 1}}\r\n      <ul class=\"currencyOption clearfix\">\r\n      {{#each currency_selector.currencies}}\r\n      <li>\r\n          <a href=\"{{{switch_url}}}\">\r\n              {{#if is_active}}\r\n                <span class=\"active\">{{{flag}}}</span>\r\n              {{else}}\r\n                <span>{{{flag}}}</span>\r\n              {{/if}}\r\n          </a>\r\n      </li>\r\n      {{/each}}\r\n      </ul>\r\n    {{/if}}\r\n\r\n    </div>\r\n    \r\n    \r\n    \r\n","components/ob-custom/ob-footer":"\r\n{{> components/ob-custom/ob-instagram}}\r\n\r\n{{> components/ob-custom/ob-newsletter}}\r\n\r\n<footer class=\"footer\" role=\"contentinfo\">\r\n    <div class=\"container\">\r\n        <div class=\"footer-wrp\">\r\n            <article class=\"footer-info-col footer-logo\">\r\n                {{#if page_type '===' 'default'}}\r\n                    <div class=\"header-logo\">\r\n                        {{> components/common/store-logo}}\r\n                    </div>\r\n                {{else}}\r\n                    <div class=\"header-logo\">\r\n                        {{> components/common/store-logo}}\r\n                    </div>\r\n                {{/if}}\r\n            </article>\r\n\r\n            <article class=\"footer-info-col footer-info-col--small\" data-section-type=\"footer-categories\">\r\n                <h5 class=\"footer-info-heading\">{{lang 'footer.categories'}}</h5>\r\n                <ul class=\"footer-info-list\">\r\n                    <li>\r\n                        <a href=\"/cookie-of-the-month/\">Featured Cookie-of-the-Month</a>\r\n                    </li>\r\n                    <li>\r\n                        <a href=\"/fresh-from-the-oven/\">Fresh from the Oven</a>\r\n                    </li>\r\n                    <li>\r\n                        <a href=\"/granola-plans\">Granola Plans</a>\r\n                    </li>\r\n                    <li>\r\n                        <a href=\"/special-occasions-corporate-gifts/\">Special Occasions & Corporate Gifts</a>\r\n                    </li>\r\n                </ul>\r\n            </article>\r\n\r\n            <article class=\"footer-info-col footer-info-col--small\" data-section-type=\"footer-webPages\">\r\n                <h5 class=\"footer-info-heading\">{{lang 'footer.navigate'}}</h5>\r\n                <ul class=\"footer-info-list\">\r\n                    <li>\r\n                        <a href=\"/our-story/\">About</a>\r\n                    </li>\r\n                    <li>\r\n                        <a href=\"/our-story/\">Our Story</a>\r\n                    </li>\r\n                    <li>\r\n                        <a href=\"/cookie-archive/\">Cookie Archive</a>\r\n                    </li>\r\n                    <li>\r\n                        <a href=\"/frequently-asked-questions/\">Frequently Asked Questions</a>\r\n                    </li>\r\n                    <li>\r\n                        <a href=\"/shipping-information/\">Shipping Information</a>\r\n                    </li>\r\n                    <li>\r\n                        <a href=\"/our-guarantee/\">Our Guarantee</a>\r\n                    </li>\r\n                    <li>\r\n                        <a href=\"/contact-us/\">Contact Us</a>\r\n                    </li>\r\n                </ul>\r\n            </article>\r\n\r\n            {{#if settings.address}}\r\n                <article class=\"footer-info-col footer-info-col--small footer-store-info\" data-section-type=\"storeInfo\">\r\n                    <h5 class=\"footer-info-heading\">{{lang 'footer.info'}}</h5>\r\n                    \r\n                    <div class=\"footer-phone-number\">\r\n                        {{#if settings.phone_number}}\r\n                            <!-- <sapn><svg><use xlink:href=\"#icon-f-phone\"></use></svg>{{lang 'footer.call_us' phone_number=settings.phone_number}}</span> -->\r\n                                <a href=\"tel:{{settings.phone_number}}\"><span> <svg><use xlink:href=\"#icon-answer\"></use></svg> {{lang 'footer.call_us' phone_number=settings.phone_number}}</span></a>\r\n                        {{/if}}\r\n                    </div>\r\n                    \r\n                    <div class=\"f-email\">\r\n                        <a href=\"mailto:sweets@tulescookies.com\"><span> <svg><use xlink:href=\"#icon-email-icon\"></use></svg> sweets@tulescookies.com </span></a>\r\n                    </div>\r\n                    \r\n                    <!-- <address>{{nl2br settings.address}}</address> -->\r\n                </article>\r\n            {{/if}}\r\n\r\n        </div>\r\n\r\n        <div class=\"footer-copyright-wrp\">\r\n            {{#if theme_settings.show_copyright_footer}}                  \r\n                <div class=\"footer-copyright f-b-b\">\r\n                    <p class=\"powered-by\">&copy; {{moment format=\"YYYY\"}}