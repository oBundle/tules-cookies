{"components/ob-custom/ob-footer":"\r\n{{> components/ob-custom/ob-instagram}}\r\n\r\n{{> components/ob-custom/ob-newsletter}}\r\n\r\n<footer class=\"footer\" role=\"contentinfo\">\r\n    <div class=\"container\">\r\n        <div class=\"footer-wrp\">\r\n            <article class=\"footer-info-col footer-logo\">\r\n                {{#if page_type '===' 'default'}}\r\n                    <div class=\"header-logo\">\r\n                        {{> components/common/store-logo}}\r\n                    </div>\r\n                {{else}}\r\n                    <div class=\"header-logo\">\r\n                        {{> components/common/store-logo}}\r\n                    </div>\r\n                {{/if}}\r\n            </article>\r\n\r\n            <article class=\"footer-info-col footer-info-col--small\" data-section-type=\"footer-categories\">\r\n                <h5 class=\"footer-info-heading\">{{lang 'footer.categories'}}</h5>\r\n                <ul class=\"footer-info-list\">\r\n                    <li>\r\n                        <a href=\"/cookie-of-the-month/\">Featured Cookie-of-the-Month</a>\r\n                    </li>\r\n                    <li>\r\n                        <a href=\"/fresh-from-the-oven/\">Fresh from the Oven</a>\r\n                    </li>\r\n                    <li>\r\n                        <a href=\"/granola-plans\">Granola Plans</a>\r\n                    </li>\r\n                    <li>\r\n                        <a href=\"/special-occasions-corporate-gifts/\">Special Occasions & Corporate Gifts</a>\r\n                    </li>\r\n                </ul>\r\n            </article>\r\n\r\n            <article class=\"footer-info-col footer-info-col--small\" data-section-type=\"footer-webPages\">\r\n                <h5 class=\"footer-info-heading\">{{lang 'footer.navigate'}}</h5>\r\n                <ul class=\"footer-info-list\">\r\n                    <li>\r\n                        <a href=\"/our-story/\">About</a>\r\n                    </li>\r\n                    <li>\r\n                        <a href=\"/our-story/\">Our Story</a>\r\n                    </li>\r\n                    <li>\r\n                        <a href=\"/cookie-archive/\">Cookie Archive</a>\r\n                    </li>\r\n                    <li>\r\n                        <a href=\"/frequently-asked-questions/\">Frequently Asked Questions</a>\r\n                    </li>\r\n                    <li>\r\n                        <a href=\"/shipping-information/\">Shipping Information</a>\r\n                    </li>\r\n                    <li>\r\n                        <a href=\"/our-guarantee/\">Our Guarantee</a>\r\n                    </li>\r\n                    <li>\r\n                        <a href=\"/contact-us/\">Contact Us</a>\r\n                    </li>\r\n                </ul>\r\n            </article>\r\n\r\n            {{#if settings.address}}\r\n                <article class=\"footer-info-col footer-info-col--small footer-store-info\" data-section-type=\"storeInfo\">\r\n                    <h5 class=\"footer-info-heading\">{{lang 'footer.info'}}</h5>\r\n                    \r\n                    <div class=\"footer-phone-number\">\r\n                        {{#if settings.phone_number}}\r\n                            <!-- <sapn><svg><use xlink:href=\"#icon-f-phone\"></use></svg>{{lang 'footer.call_us' phone_number=settings.phone_number}}</span> -->\r\n                                <a href=\"tel:{{settings.phone_number}}\"><span> <svg><use xlink:href=\"#icon-answer\"></use></svg> {{lang 'footer.call_us' phone_number=settings.phone_number}}</span></a>\r\n                        {{/if}}\r\n                    </div>\r\n                    \r\n                    <div class=\"f-email\">\r\n                        <a href=\"mailto:sweets@tulescookies.com\"><span> <svg><use xlink:href=\"#icon-email-icon\"></use></svg> sweets@tulescookies.com </span></a>\r\n                    </div>\r\n                    \r\n                    <!-- <address>{{nl2br settings.address}}</address> -->\r\n                </article>\r\n            {{/if}}\r\n\r\n        </div>\r\n\r\n        <div class=\"footer-copyright-wrp\">\r\n            {{#if theme_settings.show_copyright_footer}}                  \r\n                <div class=\"footer-copyright f-b-b\">\r\n                    <p class=\"powered-by\">&copy; {{moment format=\"YYYY\"}} {{settings.store_name}}, All Rights Reserved. </p>\r\n                </div>\r\n            {{/if}}\r\n            <div class=\"footer-social-icon f-b-b text-center\">\r\n                    {{> components/common/social-links}}\r\n            </div>\r\n            <div class=\"footer-right f-b-b text-right\">\r\n                <p class=\"powered-by\">\r\n                    <a href=\"#\">Terms & Conditions</a> |\r\n                    <a href=\"/shipping-information/\">Shipping & Returns </a> |\r\n                    <a href=\"/privacy-security/\">Privacy Policy</a> |\r\n                    <a href=\"{{urls.sitemap}}\">Sitemap</a>\r\n                </p>\r\n            </div>\r\n        </div>\r\n\r\n\r\n\r\n\r\n        {{#if theme_settings.social_icon_placement_bottom '!==' 'bottom_none'}}\r\n            <article class=\"footer-info-col\r\n                footer-info-col--social\r\n                footer-info-col--{{#if theme_settings.social_icon_placement_bottom '===' 'bottom_left'}}left{{else}}right{{/if}}\"\r\n                data-section-type=\"footer-webPages\">\r\n                    <h5 class=\"footer-info-heading\">{{lang 'social.connect'}}</h5>\r\n                    {{> components/common/social-links}}\r\n            </article>\r\n        {{/if}}\r\n\r\n\r\n        <section class=\"footer-info\">\r\n\r\n            <!-- {{#and theme_settings.shop_by_brand_show_footer shop_by_brand.length}}\r\n            <article class=\"footer-info-col footer-info-col--small\" data-section-type=\"footer-brands\">\r\n                <h5 class=\"footer-info-heading\">{{lang 'footer.brands'}}</h5>\r\n                <ul class=\"footer-info-list\">\r\n                    {{#each shop_by_brand}}\r\n                        <li>\r\n                            <a href=\"{{url}}\">{{name}}</a>\r\n                        </li>\r\n                    {{/each}}\r\n                    <li><a href=\"{{urls.brands}}\">{{lang 'common.view_all'}}</a></li>\r\n                </ul>\r\n            </article>\r\n            {{/and}} -->\r\n\r\n            \r\n            {{> components/common/payment-icons}}   \r\n        </section>\r\n        {{#if theme_settings.geotrust_ssl_common_name}}\r\n            <div class=\"footer-geotrust-ssl-seal\">\r\n                {{> components/common/geotrust-ssl-seal}}\r\n            </div>\r\n        {{/if}}\r\n        \r\n        \r\n    </div>\r\n</footer>\r\n","components/ob-custom/ob-instagram":"\r\n{{#if theme_settings.instagramKey}}\r\n<article class=\"instagram clearfix\" data-section-type=\"storeInfo\">\r\n  <div class=\"container\">\r\n    <div class=\"home-section-heading text-center\">\r\n        <!-- <img src=\"{{cdn 'img/heading-top-img.png'}}\" alt=\"\">      -->\r\n        <h2 class=\"page-heading home-heading\">#Tuleâ€™s Cookies</h2>\r\n        <!-- <span>Best Collections</span> -->\r\n    </div>\r\n      <div class=\"home-instagram align-center insta-contain\">\r\n          <div class=\"insta-block clearfix\">\r\n            {{#if theme_settings.instagramKey}}\r\n              <div id=\"instafeed\" class=\"clearfix\"\r\n              data-id=\"{{#withFirst (split theme_settings.instagramKey '.')}}{{this}}{{/withFirst}}\"\r\n              data-key=\"{{theme_settings.instagramKey}}\"\r\n              data-loading=\"{{cdn 'img/loading.svg'}}\">\r\n              </div>\r\n            {{/if}}\r\n        </div>\r\n      </div>\r\n    </div>\r\n</article>\r\n{{/if}}\r\n","components/common/store-logo":"<a href=\"{{urls.home}}\">\n    {{#if settings.store_logo.image}}\n        {{#if theme_settings.logo_size '===' 'original'}}\n            <img class=\"header-logo-image-unknown-size\" src=\"{{getImage settings.store_logo.image 'logo_size'}}\" alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\">\n        {{else}}\n            <div class=\"header-logo-image-container\">\n                <img class=\"header-logo-image\" src=\"{{getImage settings.store_logo.image 'logo_size'}}\" alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\">\n            </div>\n        {{/if}}\n    {{else}}\n        <span class=\"header-logo-text\">{{settings.store_logo.title}}</span>\n    {{/if}}\n</a>\n","components/ob-custom/ob-newsletter":"{{#if settings.show_newsletter_box}}\r\n    <div class=\"container\">\r\n        <div class=\"footer-newsletter\" data-section-type=\"newsletterSubscription\">\r\n            <div class=\"bg-img\">\r\n                <img src=\"/product_images/uploaded_images/newsletter-bg-img.jpg\" alt=\"\">\r\n                <div class=\"subscription-form\">\r\n                    <div class=\"border-wrp\"></div>\r\n                    {{> components/ob-custom/ob-subscription-form}}\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n{{/if}}\n","components/common/payment-icons":"{{#with theme_settings}}\n    {{#or show_accept_amex show_accept_discover show_accept_mastercard show_accept_paypal show_accept_visa}}\n        <div class=\"footer-payment-icons\">\n            {{#if show_accept_amex}}\n                <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-american-express\"></use></svg>\n            {{/if}}\n            {{#if show_accept_discover}}\n                <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-discover\"></use></svg>\n            {{/if}}\n            {{#if show_accept_mastercard}}\n                <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-mastercard\"></use></svg>\n            {{/if}}\n            {{#if show_accept_paypal}}\n                <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-paypal\"></use></svg>\n            {{/if}}\n            {{#if show_accept_visa}}\n                <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-visa\"></use></svg>\n            {{/if}}\n        </div>\n    {{/or}}\n{{/with}}\n","components/common/social-links":"{{#if social_media}}\n    <ul class=\"socialLinks socialLinks--alt\">\n        {{#each social_media}}\n            <li class=\"socialLinks-item\">\n                {{#if display_name '===' 'google+'}}\n                    <a class=\"icon icon--google\" href=\"{{url}}\" target=\"_blank\">\n                        <svg><use xlink:href=\"#icon-google\" /></svg>\n                    </a>\n                {{else}}\n                    <a class=\"icon icon--{{display_name}}\" href=\"{{url}}\" target=\"_blank\">\n                        <svg><use xlink:href=\"#icon-{{display_name}}\" /></svg>\n                    </a>\n                {{/if}}\n            </li>\n        {{/each}}\n    </ul>\n{{/if}}\n","components/common/geotrust-ssl-seal":"<table width=\"135\" border=\"0\" cellpadding=\"2\" cellspacing=\"0\" title=\"Click to Verify - This site chose GeoTrust SSL for secure e-commerce and confidential communications.\">\n    <tr>\n        <td width=\"135\" align=\"center\" valign=\"top\">\n            <script type=\"text/javascript\" src=\"https://seal.geotrust.com/getgeotrustsslseal?host_name={{theme_settings.geotrust_ssl_common_name}}&amp;size={{theme_settings.geotrust_ssl_seal_size}}&amp;lang={{locale_name}}\"></script><br />\n            <a href=\"http://www.geotrust.com/ssl/\" target=\"_blank\"  style=\"color:#000000; text-decoration:none; font:bold 7px verdana,sans-serif; letter-spacing:.5px; text-align:center; margin:0px; padding:0px;\"></a>\n        </td>\n    </tr>\n</table>\n","components/ob-custom/ob-subscription-form":"<!-- <h5 class=\"footer-info-heading\">{{lang 'newsletter.subscribe'}}</h5> -->\r\n<form class=\"form\" action=\"{{urls.subscribe.action}}\" method=\"post\">\r\n    <fieldset class=\"form-fieldset\">\r\n        <input type=\"hidden\" name=\"action\" value=\"subscribe\">\r\n        <input type=\"hidden\" name=\"nl_first_name\" value=\"bc\">\r\n        <input type=\"hidden\" name=\"check\" value=\"1\">\r\n        <div class=\"form-field\">\r\n            <label class=\"form-label is-srOnly\" for=\"nl_email\">{{lang 'common.email_address'}}</label>\r\n            <div class=\"form-prefixPostfix wrap\">\r\n                <input class=\"form-input\" id=\"nl_email\" name=\"nl_email\" type=\"email\" value=\"{{customer.email}}\" placeholder=\"{{lang 'newsletter.email_placeholder'}}\">\r\n                <input class=\"button button--primary form-prefixPostfix-button--postfix\" type=\"submit\" value=\"{{lang 'newsletter.subscribe_submit'}}\">\r\n            </div>\r\n            {{#if settings.show_newsletter_summary }}\r\n                <div class=\"footer-newsletter-summary\">{{settings.newsletter_summary}}</div>\r\n            {{/if}}\r\n        </div>\r\n    </fieldset>\r\n</form>\r\n<p class=\"newsletter-text\">{{lang 'newsletter.subscribe_intro'}}</p>\r\n"}