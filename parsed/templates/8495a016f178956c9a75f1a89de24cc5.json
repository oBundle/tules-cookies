{"pages/account/wishlists":"{{#partial \"page\"}}\n{{#if settings.show_wishlist}}\n    <div class=\"account account--fixedLarge\">\n        {{> components/common/breadcrumbs breadcrumbs=breadcrumbs}}\n        <h2 class=\"page-heading\">{{lang 'account.wishlists.heading' }}</h2>\n        {{> components/account/navigation account_page='wishlists'}}\n        {{> components/account/wishlist-list}}\n    </div>\n{{/if}}\n{{/partial}}\n{{> layout/base}}\n","components/common/breadcrumbs":"<ul class=\"breadcrumbs\" itemscope itemtype=\"http://schema.org/BreadcrumbList\">\n    {{#unless theme_settings.hide_breadcrumbs }}\n        {{#each breadcrumbs}}\n            <li class=\"breadcrumb {{#if @last}}is-active{{/if}}\" itemprop=\"itemListElement\" itemscope itemtype=\"http://schema.org/ListItem\">\n                {{#or @last (if url \"==\" null)}}\n                    <meta itemprop=\"item\" content=\"{{url}}\">\n                    <span class=\"breadcrumb-label\" itemprop=\"name\">{{name}}</span>\n                {{else}}\n                    <a href=\"{{url}}\" class=\"breadcrumb-label\" itemprop=\"item\"><span itemprop=\"name\">{{name}}</span></a>\n                {{/or}}\n                <meta itemprop=\"position\" content=\"{{add @index 1}}\" />\n            </li>\n        {{/each}}\n    {{/unless}}\n</ul>\n","components/account/navigation":"<nav class=\"navBar navBar--sub navBar--account\">\n    <ul class=\"navBar-section\">\n        {{#if account_page '===' 'orders'}}\n            {{#if page_type '===' 'account_order'}}\n                <li class=\"navBar-item is-active\">\n                    <a class=\"navBar-action\" href=\"{{urls.account.orders.all}}\">{{lang 'account.nav.orders'}}</a>\n                </li>\n            {{else}}\n                <li class=\"navBar-item is-active\">{{lang 'account.nav.orders'}}</li>\n            {{/if}}\n        {{else}}\n            <li class=\"navBar-item\">\n                <a class=\"navBar-action\" href=\"{{urls.account.orders.all}}\">{{lang 'account.nav.orders'}}</a>\n            </li>\n        {{/if}}\n        {{#if settings.returns_enabled}}\n            {{#if account_page '===' 'returns'}}\n                <li class=\"navBar-item is-active\">{{lang 'account.nav.returns'}}</li>\n            {{else}}\n                <li class=\"navBar-item\">\n                    <a class=\"navBar-action\" href=\"{{urls.account.returns}}\">{{lang 'account.nav.returns'}}</a>\n                </li>\n            {{/if}}\n        {{/if}}\n        {{#if account_page '===' 'messages'}}\n            <li class=\"navBar-item is-active\">{{lang 'account.nav.messages' num_new_messages=customer.num_new_messages}}</li>\n        {{else}}\n            <li class=\"navBar-item\">\n                <a class=\"navBar-action\" href=\"{{urls.account.inbox}}\">{{lang 'account.nav.messages' num_new_messages=customer.num_new_messages}}</a>\n            </li>\n        {{/if}}\n        {{#if account_page '===' 'addresses'}}\n            <li class=\"navBar-item is-active\">{{lang 'account.nav.addresses'}}</li>\n        {{else}}\n            <li class=\"navBar-item\">\n                <a class=\"navBar-action\" href=\"{{urls.account.addresses}}\">{{lang 'account.nav.addresses'}}</a>\n            </li>\n        {{/if}}\n        {{#if settings.show_payment_methods}}\n            {{#if account_page '===' 'payment_methods'}}\n                <li class=\"navBar-item is-active\">{{lang 'account.nav.payment_methods'}}</li>\n            {{else}}\n                <li class=\"navBar-item\">\n                    <a class=\"navBar-action\" href=\"{{urls.account.payment_methods.all}}\">{{lang 'account.nav.payment_methods'}}</a>\n                </li>\n            {{/if}}\n        {{/if}}\n        {{#if settings.show_wishlist}}\n            {{#if account_page '===' 'wishlists'}}\n                <li class=\"navBar-item is-active\">{{lang 'account.nav.wishlists' num_wishlists=customer.num_wishlists}}</li>\n            {{else}}\n                <li class=\"navBar-item\">\n                    <a class=\"navBar-action\" href=\"{{urls.account.wishlists.all}}\">{{lang 'account.nav.wishlists' num_wishlists=customer.num_wishlists}}</a>\n                </li>\n            {{/if}}\n        {{/if}}\n        {{#if account_page '===' 'recent_items'}}\n            <li class=\"navBar-item is-active\">{{lang 'account.nav.recently_viewed'}}</li>\n        {{else}}\n            <li class=\"navBar-item\">\n                <a class=\"navBar-action\" href=\"{{urls.account.recent_items}}\">{{lang 'account.nav.recently_viewed'}}</a>\n            </li>\n        {{/if}}\n        {{#if account_page '===' 'settings'}}\n            <li class=\"navBar-item is-active\">{{lang 'account.nav.settings'}}</li>\n        {{else}}\n            <li class=\"navBar-item\">\n                <a class=\"navBar-action\" href=\"{{urls.account.details}}\">{{lang 'account.nav.settings'}}</a>\n            </li>\n        {{/if}}\n    </ul>\n</nav>\n","components/account/wishlist-list":"<table class=\"table table--line wishlists-table\">\n    <thead class=\"table-thead\">\n        <tr>\n            <th>{{lang 'account.wishlists.name'}}</th>\n            <th>{{lang 'account.wishlists.num_items'}}</th>\n            <th>{{lang 'account.wishlists.shared'}}</th>\n            <th>{{lang 'account.wishlists.action'}}</th>\n        </tr>\n    </thead>\n    <tbody class=\"table-tbody\">\n    {{#each customer.wishlists}}\n        <tr>\n            <td><a href=\"{{view_url}}\">{{name}}</a></td>\n            <td>{{num_items}}</td>\n            <td>\n                {{#if is_public }}\n                    {{lang 'common.yes'}}\n                {{else}}\n                    {{lang 'common.no'}}\n                {{/if}}\n            </td>\n            <td class=\"table-actions\">\n                <form method=\"post\" action=\"{{delete_url}}\" class=\"form\">\n                    <fieldset class=\"form-fieldset\">\n                        {{#if is_public}}\n                            <a href=\"{{share_url}}\" class=\"button button--primary button--small\"\n                           role=\"button\">{{lang 'common.share'}}</a>\n                        {{/if}}\n                        <a href=\"{{edit_url}}\" class=\"button button--small\" data-wishlist>{{lang 'common.edit'}}</a>\n                        <input type=\"submit\" value=\"{{lang 'common.delete'}}\" class=\"button button--small\" data-wishlist-delete>\n                    </fieldset>\n                </form>\n            </td>\n        </tr>\n    {{/each}}\n    </tbody>\n</table>\n<form method=\"post\" action=\"{{this.urls.account.wishlists.delete}}\" class=\"form\">\n    {{#unless customer.wishlists}}\n        <p class=\"form-actions\">{{> components/common/alert-info (lang 'account.wishlists.you_have_none')}}</p>\n\n    {{/unless}}\n    <div class=\"form-actions\">\n        <a href=\"{{this.urls.account.wishlists.add}}\" class=\"button\" role=\"button\" data-wishlist>{{lang 'account.wishlists.new'}}</a>\n        {{inject 'wishlistDelete' (lang 'account.wishlists.delete_alert')}}\n        {{#if customer.wishlists}}\n            {{#each customer.wishlists}}\n                <input type=\"hidden\" value=\"{{this.id}}\" name=\"deletewishlist[]\">\n            {{/each}}\n            <input type=\"submit\" value=\"{{lang 'account.wishlists.delete_all'}}\" class=\"button\" data-wishlist-delete>\n        {{/if}}\n    </div>\n</form>\n","layout/base":"<!DOCTYPE html>\n<html class=\"no-js\" lang=\"{{ locale_name }}\">\n    <head>\n        <title>{{ head.title }}</title>\n        {{{ resourceHints }}}\n        {{{ head.meta_tags }}}\n        {{{ head.config }}}\n        {{#block \"head\"}} {{/block}}\n\n        <link href=\"{{ head.favicon }}\" rel=\"shortcut icon\">\n        <meta name=\"viewport\" content=\"width=device-width, initial-scale=1, maximum-scale=1\">\n\n        <script>\n            // Change document class from no-js to js so we can detect this in css\n            document.documentElement.className = document.documentElement.className.replace('no-js', 'js');\n        </script>\n\n        {{ getFontsCollection }}\n        {{{stylesheet '/assets/css/theme.css'}}}\n\n        {{{head.scripts}}}\n        {{{head.rsslinks}}}\n\n        {{inject 'themeSettings' theme_settings}}\n        {{inject 'genericError' (lang 'common.generic_error')}}\n        {{inject 'maintenanceMode' settings.maintenance}}\n        {{inject 'urls' urls}}\n        {{inject 'secureBaseUrl' settings.secure_base_url}}\n        {{inject 'cartId' cart_id}}\n        {{inject 'template' template}}\n        {{{snippet 'htmlhead'}}}\n    </head>\n    <body class=\"{{page_type}}_page\">\n        {{{snippet 'header'}}}\n        <svg data-src=\"{{cdn 'img/icon-sprite.svg'}}\" class=\"icons-svg-sprite\"></svg>\n\n        {{#and settings.privacy_cookie settings.is_eu_ip_address}}\n            {{> components/common/cookie}}\n        {{/and}}\n\n        {{> components/ob-custom/ob-header }}\n        {{> components/ob-custom/ob-body }}\n        {{> components/ob-custom/ob-footer }}\n\n        <script>window.__webpack_public_path__ = \"{{cdn 'assets/dist/'}}\";</script>\n        <script src=\"{{cdn 'assets/dist/theme-bundle.main.js'}}\"></script>\n        <script>\n            // Exported in app.js\n            window.stencilBootstrap(\"{{page_type}}\", {{jsContext}}).load();\n        </script>\n\n        {{{footer.scripts}}}\n        {{{snippet 'footer'}}}\n    </body>\n</html>\n","components/common/alert-info":"<div class=\"alertBox alertBox--info\">\n    <div class=\"alertBox-column alertBox-icon\">\n        <icon glyph=\"ic-success\" class=\"icon\" aria-hidden=\"true\"><svg xmlns=\"http://www.w3.org/2000/svg\" width=\"24\" height=\"24\" viewBox=\"0 0 24 24\"><path d=\"M12 2C6.48 2 2 6.48 2 12s4.48 10 10 10 10-4.48 10-10S17.52 2 12 2zm1 15h-2v-2h2v2zm0-4h-2V7h2v6z\"></path></svg></icon>\n    </div>\n    <p class=\"alertBox-column alertBox-message\">\n        <span>{{{this}}}</span>\n    </p>\n</div>\n","components/common/cookie":"<div class=\"cookieMessage\">\n    <div class=\"cookieMessage-container\">\n        <div class=\"cookieMessage-text\">\n            <p>{{settings.privacy_cookie}}</p>\n        </div>\n        <button class=\"button button--primary button--small\" data-privacy-accept>{{lang 'cookies.accept'}}</button>\n    </div>\n</div>\n","components/ob-custom/ob-header":"{{> components/ob-custom/ob-mobile-menu}}\r\n<header class=\"header\" role=\"banner\">\r\n        <div class=\"container\">\r\n            {{> components/ob-custom/ob-pencil-banner}}\r\n        </div>\r\n\r\n        <div class=\"header-full\">\r\n            \r\n            <div class=\"container\">\r\n                <div class=\"header-wrp\">\r\n                    <div class=\"mobileMenu-btn\">\r\n                        <a href=\"javascript:void(0);\" class=\"mobile-menu-btn align-center\">\r\n                            <span><svg><use xlink:href=\"#icon-ResponsiveMenu\"></use></svg></span>\r\n                        </a>\r\n                    </div>\r\n                    <div class=\"header-logo-wrp\">\r\n                        <div class=\"logo-desktop\">\r\n                            {{#if page_type '===' 'default'}}\r\n                                <h1 class=\"header-logo header-logo--{{theme_settings.logo-position}}\">\r\n                                    {{> components/common/store-logo}}\r\n                                </h1>\r\n                            {{else}}\r\n                                <div class=\"header-logo header-logo--{{theme_settings.logo-position}}\">\r\n                                    {{> components/common/store-logo}}\r\n                                </div>\r\n                            {{/if}}\r\n                        </div>\r\n                        <div class=\"logo-mobile\">\r\n                            <div class=\"header-logo header-logo--{{theme_settings.logo-position}}\">\r\n                                {{> components/ob-custom/ob-store-logo-mobile}}\r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n                    <div class=\"nav-menu-wrp\">\r\n                        <div class=\"navPages-container\" id=\"menu\" data-menu>\r\n                            {{> components/ob-custom/ob-navigation-menu}}\r\n                        </div>\r\n                    </div>\r\n                    <div class=\"navigation-wrp\">\r\n                        {{> components/ob-custom/ob-navigation}}\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n\r\n        \r\n        {{{region name=\"header_bottom\"}}}\r\n        \r\n    \r\n</header>\r\n","components/ob-custom/ob-body":"<div class=\"body\" data-currency-code=\"{{currency_selector.active_currency_code}}\">\r\n    {{#block \"hero\"}} {{/block}}\r\n    {{#block \"heading-full-width\"}} {{/block}}\r\n    <div class=\"container\">\r\n        {{#block \"page\"}} {{/block}}\r\n    </div>\r\n    {{#block \"full-width\"}} {{/block}}\r\n    \r\n    {{> components/common/modal}}\r\n    {{> components/common/alert-modal}}\r\n</div>\r\n    \n","components/ob-custom/ob-footer":"\r\n{{> components/ob-custom/ob-instagram}}\r\n\r\n{{> components/ob-custom/ob-newsletter}}\r\n\r\n<footer class=\"footer\" role=\"contentinfo\">\r\n    <div class=\"container\">\r\n        <div class=\"footer-wrp\">\r\n            <article class=\"footer-info-col footer-logo\">\r\n                {{#if page_type '===' 'default'}}\r\n                    <div class=\"header-logo\">\r\n                        {{> components/common/store-logo}}\r\n                    </div>\r\n                {{else}}\r\n                    <div class=\"header-logo\">\r\n                        {{> components/common/store-logo}}\r\n                    </div>\r\n                {{/if}}\r\n            </article>\r\n\r\n            <article class=\"footer-info-col footer-info-col--small\" data-section-type=\"footer-categories\">\r\n                <h5 class=\"footer-info-heading\">{{lang 'footer.categories'}}</h5>\r\n                <ul class=\"footer-info-list\">\r\n                    <li>\r\n                        <a href=\"/cookie-of-the-month/\">Featured Cookie-of-the-Month</a>\r\n                    </li>\r\n                    <li>\r\n                        <a href=\"/fresh-from-the-oven/\">Fresh from the Oven</a>\r\n                    </li>\r\n                    <li>\r\n                        <a href=\"/granola-plans\">Granola Plans</a>\r\n                    </li>\r\n                    <li>\r\n                        <a href=\"/special-occasions-corporate-gifts/\">Special Occasions & Corporate Gifts</a>\r\n                    </li>\r\n                </ul>\r\n            </article>\r\n\r\n            <article class=\"footer-info-col footer-info-col--small\" data-section-type=\"footer-webPages\">\r\n                <h5 class=\"footer-info-heading\">{{lang 'footer.navigate'}}</h5>\r\n                <ul class=\"footer-info-list\">\r\n                    <li>\r\n                        <a href=\"/our-story/\">About</a>\r\n                    </li>\r\n                    <li>\r\n                        <a href=\"/our-story/\">Our Story</a>\r\n                    </li>\r\n                    <li>\r\n                        <a href=\"/cookie-archive/\">Cookie Archive</a>\r\n                    </li>\r\n                    <li>\r\n                        <a href=\"/frequently-asked-questions/\">Frequently Asked Questions</a>\r\n                    </li>\r\n                    <li>\r\n                        <a href=\"/shipping-information/\">Shipping Information</a>\r\n                    </li>\r\n                    <li>\r\n                        <a href=\"/our-guarantee/\">Our Guarantee</a>\r\n                    </li>\r\n                    <li>\r\n                        <a href=\"/contact-us/\">Contact Us</a>\r\n                    </li>\r\n                </ul>\r\n            </article>\r\n\r\n            {{#if settings.address}}\r\n                <article class=\"footer-info-col footer-info-col--small footer-store-info\" data-section-type=\"storeInfo\">\r\n                    <h5 class=\"footer-info-heading\">{{lang 'footer.info'}}</h5>\r\n                    \r\n                    <div class=\"footer-phone-number\">\r\n                        {{#if settings.phone_number}}\r\n                            <!-- <sapn><svg><use xlink:href=\"#icon-f-phone\"></use></svg>{{lang 'footer.call_us' phone_number=settings.phone_number}}</span> -->\r\n                                <a href=\"tel:{{settings.phone_number}}\"><span> <svg><use xlink:href=\"#icon-answer\"></use></svg> {{lang 'footer.call_us' phone_number=settings.phone_number}}</span></a>\r\n                        {{/if}}\r\n                    </div>\r\n                    \r\n                    <div class=\"f-email\">\r\n                        <a href=\"mailto:sweets@tulescookies.com\"><span> <svg><use xlink:href=\"#icon-email-icon\"></use></svg> sweets@tulescookies.com </span></a>\r\n                    </div>\r\n                    \r\n                    <!-- <address>{{nl2br settings.address}}</address> -->\r\n                </article>\r\n            {{/if}}\r\n\r\n        </div>\r\n\r\n        <div class=\"footer-copyright-wrp\">\r\n            {{#if theme_settings.show_copyright_footer}}                  \r\n                <div class=\"footer-copyright f-b-b\">\r\n                    <p class=\"powered-by\">&copy; {{moment format=\"YYYY\"}} {{settings.store_name}}, All Rights Reserved. </p>\r\n                </div>\r\n            {{/if}}\r\n            <div class=\"footer-social-icon f-b-b text-center\">\r\n                    {{> components/common/social-links}}\r\n            </div>\r\n            <div class=\"footer-right f-b-b text-right\">\r\n                <p class=\"powered-by\">\r\n                    <a href=\"#\">Terms & Conditions</a> |\r\n                    <a href=\"/shipping-information/\">Shipping & Returns </a> |\r\n                    <a href=\"/privacy-security/\">Privacy Policy</a> |\r\n                    <a href=\"{{urls.sitemap}}\">Sitemap</a>\r\n                </p>\r\n            </div>\r\n        </div>\r\n\r\n\r\n\r\n\r\n        {{#if theme_settings.social_icon_placement_bottom '!==' 'bottom_none'}}\r\n            <article class=\"footer-info-col\r\n                footer-info-col--social\r\n                footer-info-col--{{#if theme_settings.social_icon_placement_bottom '===' 'bottom_left'}}left{{else}}right{{/if}}\"\r\n                data-section-type=\"footer-webPages\">\r\n                    <h5 class=\"footer-info-heading\">{{lang 'social.connect'}}</h5>\r\n                    {{> components/common/social-links}}\r\n            </article>\r\n        {{/if}}\r\n\r\n\r\n        <section class=\"footer-info\">\r\n\r\n            <!-- {{#and theme_settings.shop_by_brand_show_footer shop_by_brand.length}}\r\n            <article class=\"footer-info-col footer-info-col--small\" data-section-type=\"footer-brands\">\r\n                <h5 class=\"footer-info-heading\">{{lang 'footer.brands'}}</h5>\r\n                <ul class=\"footer-info-list\">\r\n                    {{#each shop_by_brand}}\r\n                        <li>\r\n                            <a href=\"{{url}}\">{{name}}</a>\r\n                        </li>\r\n                    {{/each}}\r\n                    <li><a href=\"{{urls.brands}}\">{{lang 'common.view_all'}}</a></li>\r\n                </ul>\r\n            </article>\r\n            {{/and}} -->\r\n\r\n            \r\n            {{> components/common/payment-icons}}   \r\n        </section>\r\n        {{#if theme_settings.geotrust_ssl_common_name}}\r\n            <div class=\"footer-geotrust-ssl-seal\">\r\n                {{> components/common/geotrust-ssl-seal}}\r\n            </div>\r\n        {{/if}}\r\n        \r\n        \r\n    </div>\r\n</footer>\r\n","components/ob-custom/ob-pencil-banner":"<div class=\"pancil-banner\">\r\n    <div class=\"container\">\r\n        <div class=\"pancil-banner-text align-center\">\r\n            <p><span>{{theme_settings.pencil-banner-red-text}}:</span> {{theme_settings.pencil-banner-text}}</p>\r\n        </div>\r\n    </div> \r\n </div>\r\n","components/ob-custom/ob-mobile-menu":"<div class=\"mobile-menu-block\">\r\n    <div class=\"mobile-menu-inner clearfix\">\r\n        \r\n        <div class=\"mobile-menu-close-btn align-right clearfix\">\r\n            <a href=\"javascript:void(0)\">\r\n            <svg><use xlink:href=\"#icon-Close-01-01\"></use></svg>\r\n            </a>\r\n        </div>\r\n\r\n        <div class=\"mobile-menu-item\">\r\n            <div class=\"sidebarBlock\">\r\n                {{> components/ob-custom/ob-navigation-hardcode}}\r\n            </div>\r\n        </div>\r\n\r\n    </div>\r\n  </div>\r\n  <div class=\"menu-open-overlay\"></div>\r\n  \n","components/common/store-logo":"<a href=\"{{urls.home}}\">\n    {{#if settings.store_logo.image}}\n        {{#if theme_settings.logo_size '===' 'original'}}\n            <img class=\"header-logo-image-unknown-size\" src=\"{{getImage settings.store_logo.image 'logo_size'}}\" alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\">\n        {{else}}\n            <div class=\"header-logo-image-container\">\n                <img class=\"header-logo-image\" src=\"{{getImage settings.store_logo.image 'logo_size'}}\" alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\">\n            </div>\n        {{/if}}\n    {{else}}\n        <span class=\"header-logo-text\">{{settings.store_logo.title}}</span>\n    {{/if}}\n</a>\n","components/ob-custom/ob-store-logo-mobile":"<a href=\"{{urls.home}}\">\r\n    <img class=\"header-logo-image\" src=\"{{cdn 'assets/img/logo-mob.png'}}\" alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\">\r\n</a>\r\n    \n","components/ob-custom/ob-navigation-menu":"<nav class=\"navPages\">\r\n    <div class=\"navPages-quickSearch\">\r\n        {{> components/common/quick-search}}\r\n    </div>\r\n    <ul class=\"navPages-list sf-menu\">\r\n        {{> components/ob-custom/ob-navigation-hardcode}}\r\n    </ul>\r\n    <ul class=\"navPages-list navPages-list--user\">\r\n        {{#if currency_selector.currencies.length '>' 1}}\r\n            <li class=\"navPages-item\">\r\n                <a class=\"navPages-action has-subMenu\" href=\"#\" data-collapsible=\"navPages-currency\" aria-controls=\"navPages-currency\" aria-expanded=\"false\">\r\n                    {{lang 'common.currency' code=currency_selector.active_currency_code}} <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\"><svg><use xmlns:xlink=\"http://www.w3.org/1999/xlink\" xlink:href=\"#icon-chevron-down\"></use></svg></i>\r\n                </a>\r\n                <div class=\"navPage-subMenu\" id=\"navPages-currency\" aria-hidden=\"true\" tabindex=\"-1\">\r\n                    <ul class=\"navPage-subMenu-list\">\r\n                        {{#each currency_selector.currencies}}\r\n                        <li class=\"navPage-subMenu-item\">\r\n                            <a class=\"navPage-subMenu-action navPages-action\" href=\"{{{switch_url}}}\">\r\n                                {{#if is_active}}\r\n                                    <strong>{{name}}</strong>\r\n                                {{else}}\r\n                                    {{name}}\r\n                                {{/if}}\r\n                            </a>\r\n                        </li>\r\n                        {{/each}}\r\n                    </ul>\r\n                </div>\r\n            </li>\r\n        {{/if}}\r\n        {{#if settings.gift_certificates_enabled}}\r\n            <li class=\"navPages-item\">\r\n                <a class=\"navPages-action\" href=\"{{urls.gift_certificate.purchase}}\">{{lang 'common.gift_cert'}}</a>\r\n            </li>\r\n        {{/if}}\r\n        {{#if customer.store_credit.value '>' 0}}\r\n            <li class=\"navPages-item\">\r\n                <a class=\"navPages-action navPages-action--storeCredit\">\r\n                    {{lang 'common.store_credit_overview' credit=customer.store_credit.formatted}}\r\n                </a>\r\n            </li>\r\n        {{/if}}\r\n   \r\n    </ul>\r\n</nav>\r\n","components/ob-custom/ob-navigation":"<nav class=\"navUser\">\r\n        \r\n    \r\n        <ul class=\"navUser-section navUser-section--alt\">\r\n            <li class=\"navUser-item\">\r\n                <a class=\"navUser-action navUser-item--compare\" href=\"{{urls.compare}}\" data-compare-nav>{{lang 'common.compare'}} <span class=\"countPill countPill--positive countPill--alt\"></span></a>\r\n            </li>\r\n            <li class=\"navUser-item navUser-navigation\">\r\n                <ul class=\"userSummary sf-menu\">\r\n                    <li class=\"usermenu\">\r\n                        <a href=\"javascript:void(0)\"><svg><use xlink:href=\"#icon-account-icon\"></use></svg></a>\r\n                        <ul class=\"nav-subs accountMenu\">\r\n                            \r\n                            <li class=\"navUser-item navUser-item--account\">\r\n                                {{#if customer}}\r\n                                    <a class=\"navUser-action\" href=\"{{urls.account.index}}\">{{lang 'common.account'}}</a>\r\n                                    <a class=\"navUser-action\" href=\"{{urls.auth.logout}}\">{{lang 'common.logout'}}</a>\r\n                                {{else}}\r\n                                    <a class=\"navUser-action\" href=\"{{urls.auth.login}}\">{{lang 'common.login'}}</a>\r\n                                    {{#if settings.account_creation_enabled}}\r\n                                        <span class=\"navUser-or\">{{lang 'common.or'}}</span> <a class=\"