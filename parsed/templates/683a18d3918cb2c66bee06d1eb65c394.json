{"components/ob-custom/ob-newsletter":"{{#if settings.show_newsletter_box}}\r\n    <div class=\"container\">\r\n        <div class=\"footer-newsletter\" data-section-type=\"newsletterSubscription\">\r\n            <div class=\"bg-img\">\r\n                <img src=\"/product_images/uploaded_images/newsletter-bg-img.jpg\" alt=\"\">\r\n                <div class=\"subscription-form\">\r\n                    <div class=\"border-wrp\"></div>\r\n                    {{> components/ob-custom/ob-subscription-form}}\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n{{/if}}\r\n","components/ob-custom/ob-subscription-form":"<!-- <h5 class=\"footer-info-heading\">{{lang 'newsletter.subscribe'}}</h5> -->\r\n<form class=\"form\" action=\"{{urls.subscribe.action}}\" method=\"post\">\r\n    <fieldset class=\"form-fieldset\">\r\n        <input type=\"hidden\" name=\"action\" value=\"subscribe\">\r\n        <input type=\"hidden\" name=\"nl_first_name\" value=\"bc\">\r\n        <input type=\"hidden\" name=\"check\" value=\"1\">\r\n        <div class=\"form-field\">\r\n            <label class=\"form-label is-srOnly\" for=\"nl_email\">{{lang 'common.email_address'}}</label>\r\n            <div class=\"form-prefixPostfix wrap\">\r\n                <input class=\"form-input\" id=\"nl_email\" name=\"nl_email\" type=\"email\" value=\"{{customer.email}}\" placeholder=\"{{lang 'newsletter.email_placeholder'}}\">\r\n                <input class=\"button button--primary form-prefixPostfix-button--postfix\" type=\"submit\" value=\"{{lang 'newsletter.subscribe_submit'}}\">\r\n            </div>\r\n            {{#if settings.show_newsletter_summary }}\r\n                <div class=\"footer-newsletter-summary\">{{settings.newsletter_summary}}</div>\r\n            {{/if}}\r\n        </div>\r\n    </fieldset>\r\n</form>\r\n<p class=\"newsletter-text\">{{lang 'newsletter.subscribe_intro'}}</p>\r\n"}
